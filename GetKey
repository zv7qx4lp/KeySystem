-- Bảng khóa theo loại (chỉ liệt kê key, script sẽ tự gán duration)
local key24h = {
    "NYRICX-6HW8-8LGZ-ASJ3-56J3-KKKD-VKG0",
    "NYRICX-P4T2-4QTY-EHYB-IJJR-OYCG-4L9E",
    "NYRICX-PJ8P-O5MA-GT00-3XCZ-JMRP-DI2Q"
}
local keyweek = {
    "W69qL7-4hR6EF-#hQzKp-WEEKLY-e$V$2i",
    "N@6z5@-WEEKLY-OmPJwi-zCBQBI-7CxPxq",
    "3^tuhC-f1KsWU-30PCWU-crsSXR-WEEKLY",
    "9262D%-WEEKLY-dmGsPO-bfRd5y-mJDMQd"
}
local keymonth = {
    "myn55q$b-obomEDeF-6H^vkVRN-MONTHLY-mU6fz$Y^",
    "1qiKy7%J-yvZ&BX$P-0&5V09Di-MONTHLY-5IXmncd5",
    "OKIWPRHa-0r1ADMmV-pzHrNdss-MONTHLY-RWyhX7vr",
    "X^D78UH@-MONTHLY-%fEI#TxO-4jBPtBC^-Fx@s3shg"
}
local keyboost = {
    "BOOST-PRH3F6-221T71B-HMKBEAR",
    "BOOST-TAM7ZR-YTVPICW-KENYC",
    "BOOST-09S18G-ODYOC4C-KIENLONA"
}

-- Ánh xạ key với duration
local key24hMap = {}
for _, key in ipairs(key24h) do
    key24hMap[key] = 86400 -- 24h
end

local keyweekMap = {}
for _, key in ipairs(keyweek) do
    keyweekMap[key] = 604800 -- 7 days
end

local keymonthMap = {}
for _, key in ipairs(keymonth) do
    keymonthMap[key] = 2592000 -- 30 days
end

local keyboostMap = {}
for _, key in ipairs(keyboost) do
    keyboostMap[key] = 0 -- Permanent
end

local getKeyLink = "https://discord.gg/7c5dZnghDe"

-- Dịch vụ
local TweenService = game:GetService("TweenService")
local SoundService = game:GetService("SoundService")
local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")

local player = Players.LocalPlayer

-- TweenInfo tái sử dụng
local tweenInfoQuadOut = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local tweenInfoQuadIn = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
local tweenInfoBackOut = TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out)
local tweenInfoShake = TweenInfo.new(0.05, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)

-- Nhạc nền
local bgMusic = Instance.new("Sound", SoundService)
bgMusic.SoundId = "rbxassetid://9043887091"
bgMusic.Volume = 0.5
bgMusic.Looped = true
bgMusic:Play()

-- Tạo ScreenGui
local screenGui = Instance.new("ScreenGui", game.CoreGui)
screenGui.Name = "NyricXHubUI"

-- Tạo giao diện chào mừng
local function createWelcomeUI()
    local welcomeFrame = Instance.new("Frame", screenGui)
    welcomeFrame.Size = UDim2.new(1, 0, 1, 0)
    welcomeFrame.BackgroundTransparency = 1

    local welcomeTextContainer = Instance.new("Frame", welcomeFrame)
    welcomeTextContainer.Size = UDim2.new(0, 500, 0, 100)
    welcomeTextContainer.Position = UDim2.new(0.5, -250, 0.4, 0)
    welcomeTextContainer.BackgroundTransparency = 1
    welcomeTextContainer.ClipsDescendants = true

    local welcomeText = Instance.new("TextLabel", welcomeTextContainer)
    welcomeText.Size = UDim2.new(1, 0, 1, 0)
    welcomeText.Text = "Welcome to NyricX Hub"
    welcomeText.TextColor3 = Color3.fromRGB(255, 223, 186)
    welcomeText.Font = Enum.Font.FredokaOne
    welcomeText.TextStrokeTransparency = 0
    welcomeText.BackgroundTransparency = 1
    welcomeText.TextScaled = true
    welcomeText.TextTransparency = 1

    local welcomeTextStroke = Instance.new("UIStroke", welcomeText)
    welcomeTextStroke.Thickness = 3
    welcomeTextStroke.Color = Color3.fromRGB(255, 180, 0)

    local gradient = Instance.new("UIGradient", welcomeText)
    gradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 223, 186)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 180, 0))
    })
    gradient.Rotation = 45

    local glowStroke = Instance.new("UIStroke", welcomeText)
    glowStroke.Thickness = 8
    glowStroke.Color = Color3.fromRGB(255, 180, 0)
    glowStroke.Transparency = 0.7

    TweenService:Create(welcomeText, TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        TextTransparency = 0,
        Rotation = 2,
        Size = UDim2.new(1.05, 0, 1.05, 0)
    }):Play()

    TweenService:Create(glowStroke, TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        Transparency = 0.3
    }):Play()

    wait(1.2)

    TweenService:Create(welcomeText, TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
        TextTransparency = 1,
        Rotation = -2,
        Size = UDim2.new(0.95, 0, 0.95, 0)
    }):Play()

    TweenService:Create(glowStroke, TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
        Transparency = 1
    }):Play()

    wait(0.8)
    welcomeFrame:Destroy()
end

-- Tạo trang trí góc
local function createCornerDecorations()
    local positions = {
        UDim2.new(0, 20, 0, 20),
        UDim2.new(1, -100, 0, 20),
        UDim2.new(0, 20, 1, -100),
        UDim2.new(1, -100, 1, -100)
    }

    for _, pos in pairs(positions) do
        local icon = Instance.new("ImageLabel", screenGui)
        icon.Size = UDim2.new(0, 80, 0, 80)
        icon.Position = pos
        icon.BackgroundTransparency = 1
        icon.Image = "rbxassetid://10994610552"
        icon.ImageTransparency = 0.4
        icon.Rotation = math.random(0, 360)
    end
end

-- Hiệu ứng pháo hoa
local function createFirework()
    for i = 1, 30 do
        local star = Instance.new("ImageLabel", screenGui)
        star.Size = UDim2.new(0, math.random(15, 30), 0, math.random(15, 30))
        star.Position = UDim2.new(math.random(), 0, math.random(), 0)
        star.Image = "rbxassetid://7712218325"
        star.BackgroundTransparency = 1
        star.ImageTransparency = 0.2
        star.ImageColor3 = Color3.fromRGB(math.random(150, 255), math.random(100, 255), math.random(100, 255))

        TweenService:Create(star, TweenInfo.new(1, Enum.EasingStyle.Exponential), {
            Position = UDim2.new(math.random(), 0, math.random(), 0),
            ImageTransparency = 1
        }):Play()

        wait(0.02)
        star:Destroy()
    end
end

-- Tải giao diện chính
local function loadMainHubUI()
    local success, result = pcall(function()
        local url = "https://raw.githubusercontent.com/xyrixreal/NyricXHub/refs/heads/main/NyricXHub"
        local scriptContent = game:HttpGet(url)
        if not scriptContent or scriptContent == "" then
            error("Failed to fetch script content from " .. url)
        end
        local loadedFunction = loadstring(scriptContent)
        if not loadedFunction then
            error("Failed to compile script from " .. url)
        end
        return loadedFunction()
    end)
    if not success then
        warn("Failed to load NyricX Hub: " .. tostring(result))
        -- Hiển thị thông báo lỗi trên UI nếu cần
        if screenGui and screenGui:FindFirstChild("MainFrame") then
            local mainFrame = screenGui.MainFrame
            local titleLabel = mainFrame:FindFirstChild("TitleLabel")
            if titleLabel then
                titleLabel.Text = "Error: Failed to load NyricX Hub"
                titleLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
            end
        end
    elseif result then
        print("Successfully loaded NyricX Hub")
    else
        warn("NyricX Hub loaded but returned no value")
    end
end

-- Lưu khóa
local function saveKey(key, duration)
    local keyData = {
        key = key,
        timestamp = os.time(),
        duration = duration
    }
    pcall(function()
        writefile("NyricXKey.txt", HttpService:JSONEncode(keyData))
    end)
end

-- Kiểm tra khóa hợp lệ
local function checkKeyValidity()
    if isfile("NyricXKey.txt") then
        local success, keyData = pcall(function()
            return HttpService:JSONDecode(readfile("NyricXKey.txt"))
        end)
        if success and keyData.key and keyData.timestamp and keyData.duration then
            local currentTime = os.time()
            local duration = keyData.duration == 0 and math.huge or keyData.duration
            local isValid = key24hMap[keyData.key] or keyweekMap[keyData.key] or keymonthMap[keyData.key] or keyboostMap[keyData.key]
            if isValid and currentTime - keyData.timestamp < duration then
                print("Key valid. Loading NyricX Hub...")
                bgMusic:Stop()
                screenGui:Destroy()
                loadMainHubUI()
                return true
            else
                pcall(function()
                    writefile("NyricXKey.txt", "")
                end)
            end
        end
    end
    return false
end

-- Kiểm tra khóa từ getgenv()
local function checkEnvKey()
    local envKeys = {
        { var = getgenv().key, maps = {key24hMap, keyweekMap, keymonthMap, keyboostMap} },
        { var = getgenv().key24h, maps = {key24hMap} },
        { var = getgenv().keyweek, maps = {keyweekMap} },
        { var = getgenv().keymonth, maps = {keymonthMap} },
        { var = getgenv().keyboost, maps = {keyboostMap} }
    }

    for _, env in ipairs(envKeys) do
        local key = env.var
        if type(key) == "string" and key ~= "" then
            for _, keyMap in ipairs(env.maps) do
                if keyMap[key] then
                    saveKey(key, keyMap[key])
                    print("Environment key valid. Loading NyricX Hub...")
                    bgMusic:Stop()
                    screenGui:Destroy()
                    loadMainHubUI()
                    return true
                end
            end
        end
    end
    return false
end

-- Tạo khung chính
local function createMainFrame()
    local frame = Instance.new("Frame", screenGui)
    frame.Name = "MainFrame"
    frame.Size = UDim2.new(0, 460, 0, 340)
    frame.Position = UDim2.new(0.5, -230, 1, 0)
    frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    frame.BackgroundTransparency = 0.15

    local uiCorner = Instance.new("UICorner", frame)
    uiCorner.CornerRadius = UDim.new(0, 12)

    local uiStroke = Instance.new("UIStroke", frame)
    uiStroke.Thickness = 4
    uiStroke.Color = Color3.fromRGB(255, 180, 0)

    local uiShadow = Instance.new("UIStroke", frame)
    uiShadow.Thickness = 6
    uiShadow.Color = Color3.fromRGB(0, 0, 0)
