    TweenService:Create(frame, tweenInfoBackOut, {Position = UDim2.new(0.5, -230, 0.5, -170)}):Play()

    local titleLabel = Instance.new("TextLabel", frame)
    titleLabel.Name = "TitleLabel"
    titleLabel.Size = UDim2.new(0.8, 0, 0, 60)
    titleLabel.Position = UDim2.new(0.1, 0, 0.05, 0)
