-- Thực thi chính
createWelcomeUI()
createCornerDecorations()

if checkKeyValidity() or checkEnvKey() then
    return
end

local mainFrame, titleLabel, textBox, submitButton, closeButton = createMainFrame()
local confirmFrame, yesButton, noButton = createConfirmFrame()

local function showConfirm()
    confirmFrame.Visible = true
    mainFrame.Visible = false
    TweenService:Create(confirmFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        BackgroundTransparency = 0.2,
        Size = UDim2.new(0, 420, 0, 210)
    }):Play()
end

closeButton.MouseButton1Click:Connect(showConfirm)

yesButton.MouseButton1Click:Connect(function()
    TweenService:Create(confirmFrame, tweenInfoQuadIn, {
        BackgroundTransparency = 1,
        Size = UDim2.new(0, 380, 0, 190)
    }):Play()
    wait(0.3)
    screenGui:Destroy()
    bgMusic:Stop()
end)

noButton.MouseButton1Click:Connect(function()
    TweenService:Create(confirmFrame, tweenInfoQuadIn, {
        BackgroundTransparency = 1,
        Size = UDim2.new(0, 380, 0, 190)
    }):Play()
    wait(0.3)
    confirmFrame.Visible = false
    mainFrame.Visible = true
end)

local function submitKey()
    local inputKey = textBox.Text:match("^%s*(.-)%s*$") -- Loại bỏ khoảng trắng
    if inputKey == "" then
        titleLabel.Text = "Please enter a key!"
        titleLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
        wait(1.5)
        titleLabel.Text = "Key System for NyricX Hub"
        titleLabel.TextColor3 = Color3.fromRGB(255, 215, 0)
        return
    end

    local duration = key24hMap[inputKey] or keyweekMap[inputKey] or keymonthMap[inputKey] or keyboostMap[inputKey]
    if duration then
        titleLabel.Text = "Key Verified! Starting NyricX Hub..."
        titleLabel.TextColor3 = Color3.fromRGB(0, 255, 127)

        saveKey(inputKey, duration)

        TweenService:Create(mainFrame, TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            BackgroundTransparency = 1,
            Size = UDim2.new(0, 480, 0, 360)
        }):Play()

        wait(0.5)
        mainFrame.Visible = false
        createFirework()

        print("Key valid. Loading NyricX Hub...")
        bgMusic:Stop()
        wait(0.5)
        screenGui:Destroy()
        loadMainHubUI()
    else
        titleLabel.Text = "Invalid Key! Try Again."
        titleLabel.TextColor3 = Color3.fromRGB(255, 50, 50)

        for i = 1, 4 do
            TweenService:Create(mainFrame, tweenInfoShake, {
                Position = UDim2.new(0.5, -230 + math.random(-5, 5), 0.5, -170)
            }):Play()
            wait(0.05)
        end

        wait(1.5)
        titleLabel.Text = "Key System for NyricX Hub"
        titleLabel.TextColor3 = Color3.fromRGB(255, 215, 0)
    end
end

submitButton.MouseButton1Click:Connect(submitKey)

textBox.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        submitKey()
    end
end)
