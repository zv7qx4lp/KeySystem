    local confirmText = Instance.new("TextLabel", confirmFrame)
    confirmText.Size = UDim2.new(1, 0, 0.4, 0)
    confirmText.Text = "Are you sure you want to exit?"
    confirmText.TextSize = 22
    confirmText.Font = Enum.Font.FredokaOne
