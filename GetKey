    closeButton.MouseLeave:Connect(function()
        TweenService:Create(closeButton, tweenInfoQuadOut, {
            BackgroundColor3 = Color3.fromRGB(200, 0, 0),
            Size = UDim2.new(0, 40, 0, 40)
        }):Play()
