end

-- Tạo khung xác nhận
local function createConfirmFrame()
    local confirmFrame = Instance.new("Frame", screenGui)
    confirmFrame.Size = UDim2.new(0, 400, 0, 200)
    confirmFrame.Position = UDim2.new(0.5, -200, 0.5, -100)
    confirmFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    confirmFrame.BackgroundTransparency = 0.2
    confirmFrame.Visible = false

    local confirmCorner = Instance.new("UICorner", confirmFrame)
    confirmCorner.CornerRadius = UDim.new(0, 12)

